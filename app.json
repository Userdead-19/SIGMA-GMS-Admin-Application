{
  "expo": {
    "name": "SIGMA-GMS-Admin-Application",
    "slug": "SIGMA-GMS-Admin-Application",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sigmalogowithbg.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sigmalogowithbg.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gaming_dood.SIGMAGMSAdminApplication"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e1bbedca-6529-49aa-962d-1eb8b9e8e170"
      }
    }
  }
}
